        <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 预处理程序" enabled="true">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import org.apache.commons.lang3.RandomStringUtils

// 1. 定义Base62字符集（0-9a-zA-Z顺序）
def base62Chars = (&apos;0&apos;..&apos;9&apos;) + (&apos;a&apos;..&apos;z&apos;) + (&apos;A&apos;..&apos;Z&apos;) as char[]

// 2. 生成前三位（pos1-pos3）和后三位（pos5-pos7）
def partA = RandomStringUtils.random(3, 0, 0, true, true, base62Chars)
def partB = RandomStringUtils.random(3, 0, 0, true, true, base62Chars)

// 3. 计算校验位（pos4）
int[] weights = [5, 67, 23, 71, 73, 79] // 前3位权重 + 后3位权重
int sum = 0

// 前3位加权（权重5,67,23）
(0..2).each { index -&gt;
    int value = base62Chars.findIndexOf { it == partA.charAt(index) }
    sum += value * weights[index]
}

// 后3位加权（权重71,73,79）
(0..2).each { index -&gt;
    int value = base62Chars.findIndexOf { it == partB.charAt(index) }
    sum += value * weights[index + 3]
}

// 计算校验位
int checksum = sum % 62
char checksumChar = base62Chars[checksum]

// 4. 组合完整字符串（7位）
def finalCode = &quot;${partA}${checksumChar}${partB}&quot;

// 5. 存储到JMeter变量
vars.put(&quot;validCode&quot;, finalCode)
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="TestPlan.comments">生成路径参数</stringProp>
        </JSR223PreProcessor>
